{
    "manifest_version": 2,
    "name": "FixMe Extension",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery-3.4.1.min.js","content.js"]
        }
    ],
    "browser_action":{
        "default_icon": "fixme_icon.png"
    },
    "background": {
        "scripts": ["background.js"]
    }
}